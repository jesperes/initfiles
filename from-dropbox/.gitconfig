[color]
	interactive = always
	status = auto
	diff = auto
[user]
	name = Jesper Eskilson
	email = jesper.eskilson@iar.com

[alias]
	st = status -s -b
	co = commit
	lg = !git --no-pager log --graph --oneline --decorate --date=relative -n 25

[gui]
	fontdiff = -family \"Ubuntu Mono\" -size 10 -weight normal -slant roman -underline 0 -overstrike 0
	recentrepo = /home/jesperes/dev/eclipse
	fontui = -family Ubuntu -size -12 -weight normal -slant roman -underline 0 -overstrike 0
[diff]
	tool = tkdiff
	guitool = tkdiff

[difftool]
	prompt = false
[svn]
	# authorsprog = /home/jesperes/init/ldap-author-search.sh
	authorsfile = /home/jesperes/.gitsvnauthors
[help]
	format = web
[web]
	browser = chromium
[giggle]
	main-window-maximized = true
	main-window-geometry = 1491x1000+2149+82
	main-window-view = HistoryView
	history-view-vpane-position = 331
	file-view-vpane-position = 952
